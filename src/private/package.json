{
  "private": true,
  "name": "spacebeaver",
  "version": "0.0.1",
  "engines": {
    "node": "16.15.0",
    "npm": "6.14.17"
  },
  "scripts": {
    "test": "cross-env NODE_ENV=test jest",
    "build": "cross-env NODE_ENV=production webpack --config webpack/config.js --profile --color",
    "lint": "cross-env NODE_ENV=development node_modules/.bin/eslint --fix --ext .jsx,.js src/",
    "start": "cross-env NODE_ENV=development webpack-dashboard -c blue --title spacebeaver -- webpack-dev-server --mode development --color --config webpack/config.js"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "moduleNameMapper": {
      "^typeface-": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/fileMock.jsx",
      "\\.(css|styl|less|sass|scss)$": "<rootDir>/src/__mocks__/styleMock.jsx",
      "^@src(.*)$": "<rootDir>/src$1"
    },
    "setupFiles": [
      "<rootDir>/src/__tests__/setupTests.jsx"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/fixtures/",
      "setupTests.jsx"
    ],
    "collectCoverage": true
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.7.2",
    "@sentry/browser": "^6.17.6",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.2",
    "@babel/helper-module-imports": "^7.16.7",
    "@babel/plugin-proposal-do-expressions": "^7.16.7",
    "@babel/plugin-proposal-export-namespace-from": "^7.16.7",
    "@babel/plugin-proposal-throw-expressions": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@svgr/webpack": "^6.2.1",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.6",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.2.4",
    "core-js": "^3.21.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.6.0",
    "envalid": "^7.2.2",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^8.8.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-webpack-plugin": "^3.1.1",
    "exports-loader": "^3.1.0",
    "favicons": "^6.2.2",
    "favicons-webpack-plugin": "^5.0.2",
    "html-loader": "^3.1.0",
    "html-webpack-harddisk-plugin": "^2.0.0",
    "html-webpack-plugin": "^5.5.0",
    "identity-obj-proxy": "^3.0.0",
    "imports-loader": "^3.1.1",
    "jest": "^27.5.1",
    "mini-css-extract-plugin": "^2.5.3",
    "moment-locales-webpack-plugin": "^1.2.0",
    "prettier": "^2.5.1",
    "react-test-renderer": "^17.0.2",
    "regenerator-runtime": "^0.13.9",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.49.7",
    "sass-loader": "^12.4.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.1",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2",
    "webpack-dashboard": "^3.3.7",
    "webpack-dev-server": "^4.7.4",
    "webpack-merge": "^5.8.0",
    "webpackbar": "^5.0.2"
  }
}
